{
  "name": "@avanio/logger-like",
  "version": "0.2.11",
  "description": "Logger like interface and tools",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "scripts": {
    "build": "tsup src/index.mts --minify --sourcemap --format cjs,esm --dts --clean",
    "test": "vitest test --run --no-isolate --coverage",
    "coverage": "vitest test --run --no-isolate --reporter=dot --coverage --coverage.reporter=lcov",
    "lint": "eslint . --ext .mts",
    "validate": "tsc --noEmit --project tsconfig.test.json",
    "prepublishOnly": "pnpm run build"
  },
  "files": [
    "dist"
  ],
  "repository": "github:mharj/logger-like",
  "keywords": [
    "logger"
  ],
  "author": "mharj",
  "license": "MIT",
  "devDependencies": {
    "@cspell/eslint-plugin": "^8.17.5",
    "@eslint/js": "^9.23.0",
    "@stylistic/eslint-plugin": "^4.2.0",
    "@stylistic/eslint-plugin-ts": "^4.2.0",
    "@tsconfig/node18": "^18.2.4",
    "@types/node": "^22.13.13",
    "@types/sinon": "^17.0.4",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "@vitest/coverage-v8": "^3.0.9",
    "c8": "^10.1.3",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-import-resolver-typescript": "^4.2.4",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsdoc": "^50.6.9",
    "eslint-plugin-prettier": "^5.2.5",
    "eslint-plugin-sonarjs": "^3.0.2",
    "prettier": "^3.5.3",
    "sinon": "^20.0.0",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "tsup": "^8.4.0",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.28.0",
    "vite": "^6.2.3",
    "vitest": "^3.0.9"
  },
  "packageManager": "pnpm@10.6.3+sha512.bb45e34d50a9a76e858a95837301bfb6bd6d35aea2c5d52094fa497a467c43f5c440103ce2511e9e0a2f89c3d6071baac3358fc68ac6fb75e2ceb3d2736065e6",
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  }
}
